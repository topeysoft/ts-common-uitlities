{
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules/**/*",
        "**/*.spec.ts"
    ]
}